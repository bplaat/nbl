fn myfunc() {
    let x: int = 10;
    throw Exception('My custom exception');
    x += 50;
}

let x = 0;
try {
    myfunc();
} catch (const exception) {
    assert(exception.error == 'My custom exception');
    assert(exception.line == 3);
    assert(exception.column == 20);
} finally {
    x = 20;
}
assert(x == 20);
