// Helper function
fn assertFails(callback) {
    let fails = false;
    try {
        callback();
    } catch (const exception) {
        fails = true;
    }
    assert(fails);
}

const x = 10;
assert(x == 10);
assertFails(fn () {
    x = 5;
});

const str = 'Hello!';
assert(str[1] == 'e');
assert(str[100] == null);

const numbers = [0, 2, 4, 8];
assert(numbers[1] == 2);
assert(numbers[10] == null);

const person = {
    name = 'Bastiaan',
    age = 20
};
assert(person.name == 'Bastiaan');
const age = 'age';
assert(type(person[age]) == 'int');

let y = 5;
y = 10;
assert(y == 10);

let name: string = 'hoi';
assertFails(fn () {
    name = 10.5;
});
